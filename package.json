{
  "name": "binaryen",
  "description": "JavaScript version of Binaryen, a compiler infrastructure and toolchain library for WebAssembly.",
  "version": "89.0.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AssemblyScript/binaryen.js.git"
  },
  "keywords": [
    "webassembly",
    "wasm"
  ],
  "main": "index.js",
  "typings": "index.d.ts",
  "bin": {
    "wasm-opt": "bin/wasm-opt"
  },
  "scripts": {
    "readme": "doctoc API.md --github --title \"### Contents\" && node scripts/api",
    "check": "tsc index.d.ts --noEmit --strict --noImplicitAny --strictNullChecks --listFiles --diagnostics",
    "test": "npm run check && node tests/sanity && node tests/example"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "package.json",
    "package-lock.json",
    "README.md",
    "bin/wasm-opt"
  ],
  "devDependencies": {
    "dateformat": "^2.2.0",
    "doctoc": "^1.4.0",
    "semver": "^5.7.1",
    "simple-git": "^1.126.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {}
}
