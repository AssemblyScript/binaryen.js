{
  "name": "binaryen",
  "description": "JavaScript version of Binaryen, a compiler infrastructure and toolchain library for WebAssembly.",
  "version": "102.0.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AssemblyScript/binaryen.js.git"
  },
  "keywords": [
    "webassembly",
    "wasm"
  ],
  "type": "module",
  "main": "index.js",
  "typings": "index.d.ts",
  "exports": {
    ".": "./index.js",
    "./js": "./index.js",
    "./wasm": "./wasm.js"
  },
  "bin": {
    "wasm-opt": "bin/wasm-opt",
    "wasm2js": "bin/wasm2js"
  },
  "scripts": {
    "readme": "doctoc README.md --github --title \"### Contents\"",
    "check": "tsc index.d.ts --noEmit --strict --noImplicitAny --strictNullChecks --listFiles --diagnostics",
    "test:js": "node tests/sanity && node tests/example",
    "test:wasm": "node tests/sanity --wasm",
    "test": "npm run check && npm run test:js && npm run test:wasm"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "wasm.js",
    "wasm.d.ts",
    "package.json",
    "package-lock.json",
    "README.md",
    "bin/package.json",
    "bin/wasm-opt",
    "bin/wasm2js"
  ],
  "devDependencies": {
    "dateformat": "^5.0.2",
    "doctoc": "^2.1.0",
    "semver": "^7.1.3",
    "simple-git": "^2.47.0",
    "typescript": "^4.4.4"
  }
}
